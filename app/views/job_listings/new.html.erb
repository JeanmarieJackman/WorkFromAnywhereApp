<%= form_for( @job_listing , remote: true) do |f| %>
  <%f.label :title%>
  <%f.text_field :title%>
  <%f.label :description%>
  <%f.text_area :description%>
  <%f.label :duration%>
  <%f.text_field :duration%>
  <%= hidden_field_tag "job_listing[hiring_manager_id]", @current_user.id) %>
  <%f.submit%>
<% end %>

