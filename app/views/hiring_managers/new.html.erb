<%= form_for( @hiring_manager , remote: true) do |f| %>
  <%=f.label :name%>
  <%=f.text_field :name,:required=>true%>
  <%=f.label :company%>
  <%=f.text_field :company,:required=>true%>
  <%=f.label :phone%>
  <%=f.text_field :phone,:required=>true%>
  <%=f.label :address%>
  <%=f.text_field :address,:required=>true%>
  <%=f.label :fax%>
  <%=f.text_field :fax%>
  <%=f.hidden_field :email%>
  <%=f.hidden_field :password%>
  <%=f.submit%>
<% end %>