<%= form_for( @hiring_manager , remote: true) do |f| %>
  <%=f.label :name%>
  <%=f.text_field :name%>
  <%=f.label :company%>
  <%=f.text_field :company%>
  <%=f.label :phone%>
  <%=f.text_field :phone%>
  <%=f.label :address%>
  <%=f.text_field :address%>
  <%=f.label :fax%>
  <%=f.text_field :fax%>
  <%=f.hidden_field :email%>
  <%=f.hidden_field :password%>
  <%=f.submit%>
<% end %>